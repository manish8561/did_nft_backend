        "/users/verifying": {
            "post": {
                "description": "post addresses for verification",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "user"
                ],
                "summary": "post addresses for verification",
                "parameters": [
                    {
                        "description": "Addresses Data",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/users.AddressesDataValidator"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "clients.ClientStatusValidator": {
            "type": "object",
            "required": [
                "id",
                "status"
            ],
            "properties": {
                "id": {
                    "type": "integer"
                },
                "status": {
                    "type": "string",
                    "maxLength": 50
                }
            }
        },
        "clients.ClientValidator": {
            "type": "object",
            "required": [
                "email"
            ],
            "properties": {
                "email": {
                    "type": "string",
                    "maxLength": 100
                }
            }
        },
        "users.AddressesDataValidator": {
            "type": "object",
            "required": [
                "addresses",
                "username"
            ],
            "properties": {
                "addresses": {
                    "type": "array",
                    "items": {}
                },
                "username": {
                    "type": "string",
                    "maxLength": 20
                }
            }
        },
        "users.UserDecentrializeValidator": {
            "type": "object",
            "required": [
                "address",
                "data",
                "signature",
                "username"
            ],
            "properties": {
                "address": {
                    "type": "string"
                },
                "data": {
                    "type": "string"
                },
                "signature": {
                    "type": "string"
                },
                "username": {
                    "type": "string"
                }
            }
        },
        "users.UserValidator": {
            "type": "object",
            "required": [
                "network",
                "username"
            ],
            "properties": {
                "network": {
                    "type": "string",
                    "maxLength": 50
                },
                "username": {
                    "type": "string",
                    "maxLength": 20
                }
            }
        }
    },
    "securityDefinitions": {
        "ApiKeyAuth": {
            "description": "Access key for creating identity",
            "type": "apiKey",
            "name": "X-ACCESS-KEY",
            "in": "header"
        },
        "ApiKeyAuthAdmin": {
            "description": "Access key for creating client",
            "type": "apiKey",
            "name": "ADMIN-ACCESS-KEY",
            "in": "header"
        }
    }
}