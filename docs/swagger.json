{
    "swagger": "2.0",
    "info": {
        "description": "This is a ancrypto-sdk backend server for user identity.",
        "title": "Ancrypto NFT Backend Swagger API",
        "termsOfService": "https://www.ancrypto.io/terms-of-service/",
        "contact": {
